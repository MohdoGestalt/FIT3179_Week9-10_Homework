{
    "height": 400,
    "width": 380,
    "title": "Speed and Latency Test Results in Malaysian States",
    "params": [
      {
        "name": "State",
        "value": ["Kuala Lumpur"],
        "bind": {
          "input": "select",
          "options": [
            "Johor", "Kedah", "Kelantan", "Kuala Lumpur", "Labuan",
            "Melaka", "Negeri Sembilan", "Pahang", "Perak", "Perlis",
            "Pulau Penang", "Putrajaya", "Sabah", "Sarawak", "Selangor", "Terengganu"
          ],
          "multiple": true
        }
      }
    ],
    "data": {
      "url": "https://raw.githubusercontent.com/MohdoGestalt/FIT3179_Week9-10_Homework/refs/heads/main/ookla_with_state.csv"
    },
    "transform": [
      {"filter": "datum.avg_lat_ms > 0"},
      {"filter": "indexof(State, datum.name) >= 0"}
    ],
    "mark": {"type": "circle", "size": 5},
    "encoding": {
      "x": {
        "field": "avg_d_kbps",
        "type": "quantitative",
        "title": "Average Download Speed (kbps)",
        "scale": {"type": "log"}
      },
      "y": {
        "field": "avg_lat_ms",
        "type": "quantitative",
        "title": "Average Latency (ms)",
        "scale": {"type": "log"}
      },
      "color": {
        "field": "name",
        "type": "nominal",
        "title": "State of Malaysia"
      }
    }
  }
  